{
  "C_Cpp.default.configurationProvider": "ms-vscode.makefile-tools",
  "C_Cpp.default.intelliSenseMode": "gcc-x64",
  "C_Cpp.default.compilerPath": "/usr/bin/gcc",
  "C_Cpp.default.includePath": [
    "${workspaceFolder}/**",
    "/lib/modules/6.5.0-44-generic/build/include",
    "/lib/modules/6.5.0-44-generic/build/arch/x86/include",
    "/lib/modules/6.5.0-44-generic/build/arch/x86/include/generated",
    "/lib/modules/6.5.0-44-generic/build/include/uapi",
    "/lib/modules/6.5.0-44-generic/build/arch/x86/include/uapi"
  ],
  "C_Cpp.default.defines": ["DEBUG", "MODULE"],
  "C_Cpp.default.cStandard": "c11",
  "C_Cpp.default.cppStandard": "c++17",
  "files.associations": {
    "*.h": "cpp"
  },
  "editor.formatOnSave": true,
  "editor.tabSize": 4,
  "editor.insertSpaces": true,
  "terminal.integrated.shell.linux": "/bin/bash",
  "files.exclude": {
    "**/build/": true,
    "**/out/": true
  },
  "search.exclude": {
    "**/build/": true,
    "**/out/": true
  }
}
